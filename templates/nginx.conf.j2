# {{ ansible_managed }}

user www-data www-data;
worker_processes 1;

events {
  worker_connections 1024;
}

http {
  include mime.types;
  default_type application/octet-stream;

  access_log off;
  error_log /var/log/nginx/error.log;

  sendfile on;
  keepalive_timeout 65;

  gzip on;

  include sites-enabled/*.conf;
}
